<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Salim Alam</Authors>
    <Company>AlamWorks Consultation</Company>
    <Product>FluentBlob</Product>
    <PackageId>FluentBlob</PackageId>
    <Description>FluentBlob is an API  that provides a simple fluent interface to access features of Microsoft Azure Storage Blob services.</Description>
    <Copyright>(c) AlamWorks Consultation</Copyright>
    <RepositoryType>Git</RepositoryType>
    <RepositoryUrl>https://github.com/salimdeveloper/FluentBlob/</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Azure.Storage.Blob" Version="11.1.1" />
  </ItemGroup>

</Project>
